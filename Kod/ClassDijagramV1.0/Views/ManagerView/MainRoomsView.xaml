<UserControl x:Class="ClassDijagramV1._0.Views.ManagerView.MainRoomsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClassDijagramV1._0.Views.ManagerView"
             xmlns:viewModel="clr-namespace:ClassDijagramV1._0.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="655" d:DesignWidth="1280"
             Background="#D9D4CC"
             FontFamily="Berlin Sans FB Demi">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="980"/>
            <ColumnDefinition Width="150"/>
        </Grid.ColumnDefinitions>
        <DockPanel Grid.Column="1">
            <ContentControl
                        Content="{Binding CurrentRoomView}"/>
        </DockPanel>
    </Grid>
</UserControl>
